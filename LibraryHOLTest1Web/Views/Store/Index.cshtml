@model IEnumerable<LibraryHOLTest1Web.Models.Category>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p> Select one of the @Model.Count() Categories: </p>
<ul>
    @foreach (var category in Model)
    {
        <li>@Html.ActionLink(category.CategoryName, "Browse", new { category = category.CategoryName})</li>
    }
</ul>